{
  "npmScope": "atlascodedev",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "frontend": {
      "tags": []
    },
    "frontend-components": {
      "tags": []
    },
    "frontend-forms": {
      "tags": []
    },
    "frontend-hooks": {
      "tags": []
    },
    "frontend-svgs": {
      "tags": []
    },
    "frontend-jss-mixins": {
      "tags": []
    },
    "frontend-pages": {
      "tags": []
    },
    "frontend-sections": {
      "tags": []
    },
    "frontend-services": {
      "tags": []
    },
    "frontend-smoothscroll": {
      "tags": []
    },
    "frontend-theme": {
      "tags": []
    },
    "frontend-utility": {
      "tags": []
    }
  }
}
